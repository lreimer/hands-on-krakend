{
  "version": 2,
  "name": "Hands on KrakenD",
  "port": 8080,
  "cache_ttl": "3600s",
  "timeout": "3s",
  "plugin": {
    "folder": "/etc/krakend/plugins/",
    "pattern": ".so"
  },
  "extra_config": {
      "github_com/devopsfaith/krakend-gologging": {
        "level":  "DEBUG",
        "prefix": "[KRAKEND]",
        "syslog": false,
        "stdout": true
      }
    },
    "endpoints": [
          {
              "endpoint": "/",
              "method": "GET",
              "output_encoding": "no-op",
              "backend": [
                  {
                      "host": [
                          "https://www.google.com"
                      ],
                      "url_pattern": "/",
                      "encoding": "no-op",
                      "extra_config": {
                          "github_com/devopsfaith/krakend/transport/http/server/handler": {
                              "name": "example-plugin"
                          }
                      }
                  }
              ]
          }
      ]
  }
